<h1>Categorias</h1>
<ul>
  <% for (let i=0; i < categories.length; i++) { %>
    <li>
      <%= categories[i].name %>
    </li>
    <% } %>
</ul>
<div id="pagination">
  <% for (let i=0; i < lastpage; i++){ %>
    <a class="page <%=  +page === i + 1  ? 'active' : '' %>" href="/admin/categories=<%= i+1 %>">
      <%= i + 1 %>
    </a>
    <%} %><br />
</div>

<a class="register" href="/admin/categories/new">Cadastrar nova categoria</a>

<link rel="stylesheet" href="/stylesheets/listProduct.css">
